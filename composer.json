{
    "name": "shotamestiashvili/laravel-rabbitmq",
    "description": "Simple Rabbitmq package for Laravel",
    "keywords": [
        "Shota Mestiashvili",
        "laravel",
        "laravel-rabbitmq"
    ],
    "homepage": "https://github.com/shotamestiashvili/laravel-rabbitmq",
    "license": "MIT",
    "authors": [
        {
            "name": "Shota Mestiashvili",
            "email": "shotamestiashvili@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0",
        "ext-json": "*",
        "php-amqplib/php-amqplib": "^v3.2"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8",
        "orchestra/testbench": "^8.8",
        "pestphp/pest": "^2.20",
        "pestphp/pest-plugin-arch": "^2.5",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelRabbitmq\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-laravel-rabbitmq --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "laravel-rabbitmq\\LaravelRabbitmq\\LaravelRabbitmqServiceProvider"
            ],
            "aliases": {
                "LaravelRabbitmq": "laravel-rabbitmq\\LaravelRabbitmq\\Facades\\LaravelRabbitmq"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
